{
  "extends": [
    "config:base"
  ],
  "timezone": "Asia/Yangon",
  "schedule": [
    "after 1am and before 7am every weekday",
    "after 1am and before 7am every weekend"
  ],
  "automerge": true,
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "packagePatterns": [
        "^@angular\/.*"
      ],
      "groupName": "angular"
    }
  ]
}
